<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="360.0" prefWidth="350.0" style="-fx-background-color: #fff5eb;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SitWellFXAPACHE.StartupController">
   <children>
      <Label fx:id="minutes1" layoutY="-1.0" prefHeight="135.0" prefWidth="351.0" text="By default, you are going to use a dynamic timer, which changes the timer length according to your progress. Would you prefer the time to be static?" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <ImageView fitHeight="38.0" fitWidth="114.0" layoutX="209.0" layoutY="301.0" onMouseClicked="#onClickBack">
         <image>
            <Image url="@button_lets-begin.png" />
         </image>
      </ImageView>
      <ImageView fx:id="minutes" fitHeight="47.0" fitWidth="126.0" layoutX="111.0" layoutY="234.0">
         <image>
            <Image url="@button_minutes.png" />
         </image>
      </ImageView>
      <TextField fx:id="textfield" layoutX="118.0" layoutY="244.0" prefHeight="28.0" prefWidth="44.0" style="-fx-background-color: #fff5eb #fff5eb; -fx-prompt-text-fill: black;">
         <font>
            <Font name="Montserrat SemiBold" size="14.0" />
         </font>
      </TextField>
      <ImageView fx:id="button" fitHeight="47.0" fitWidth="218.0" layoutX="65.0" layoutY="150.0" onMouseClicked="#isStatic">
         <image>
            <Image url="@button_switch-to-static-timer.png" />
         </image>
      </ImageView>
      <Label fx:id="text" layoutX="97.0" layoutY="208.0" prefHeight="17.0" prefWidth="154.0" text="Timer Model Switched">
         <font>
            <Font name="Montserrat SemiBold" size="13.0" />
         </font>
      </Label>
     
   </children>
</AnchorPane>
