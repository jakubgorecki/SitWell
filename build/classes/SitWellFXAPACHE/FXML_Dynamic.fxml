<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="350.0" prefWidth="350.0" style="-fx-background-color: #fff5eb;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SitWellFXAPACHE.DynamicController">
   <children>
      <Label fx:id="timer" layoutX="58.0" layoutY="94.0" prefHeight="74.0" prefWidth="234.0" text="00:00:00" textAlignment="JUSTIFY" textFill="#1f5e1e">
         <font>
            <Font name="Arial Black" size="50.0" />
         </font></Label>
      <Pane layoutY="242.0" prefHeight="118.0" prefWidth="350.0" style="-fx-background-color: #1f5e1e;">
         <children>
            <ImageView fx:id="pauseResume" fitHeight="80.0" fitWidth="80.0" layoutX="135.0" layoutY="19.0" onMouseClicked="#onClickResume" preserveRatio="true">
               <image>
                  <Image url="@icons8-pause-96.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="80.0" fitWidth="80.0" layoutX="233.0" layoutY="19.0" onMouseClicked="#onClickSettings">
               <image>
                  <Image url="@icons8-settings-96.png" />
               </image>
            </ImageView>
            <ImageView fx:id="startStop" fitHeight="80.0" fitWidth="80.0" layoutX="37.0" layoutY="19.0" onMouseClicked="#onClickEvent" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@icons8-play-96.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </children>
</AnchorPane>
